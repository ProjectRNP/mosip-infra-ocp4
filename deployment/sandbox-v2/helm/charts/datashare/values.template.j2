component: datashare
part-of: mosip
activeProfileEnv: mz 
springConfigLabel:  {{ config_repo.version }}
configServerUrl: {{ config_server.internal_url }}
artifactoryUrl: {{ artifactory_url }} 
probe:
  initialDelay: 30  # Seconds
  period: 30 # Seconds
imagePullPolicy: Always

isGlowroot: {{ is_glowroot }} 

dockerHub:
  private: {{ docker_hub.private }}  
  keyname: {{ docker_hub.keyname }}

services:
  datashare:
    serviceName: {{ versions.services.datashare.service.key }}
    dockerImage: {{ versions.services.datashare.service.value }}
    containerPort: 8097
    servicePort: 80
    ingressPath: /v1/datashare
    healthUrl: /v1/datashare/actuator/health
